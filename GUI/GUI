            jLabel8.setVisible(true);
        }else{
            jLabel8.setVisible(false);
        }
    }                                      

    private void jLabel10PropertyChange(java.beans.PropertyChangeEvent evt) {                                        
        // TODO add your handling code here:
        if(sRight)
        {
            jLabel10.setVisible(true);
        }else{
            jLabel10.setVisible(false);
        }
    }                                       

