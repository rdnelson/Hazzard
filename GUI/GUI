    private void jTextField2ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        if(fFinish)
        {
            jTextField8.setText(jTextField3.getText());
        }else if(sFinish){
            jTextField11.setText(jTextField3.getText());
        }else{                
        }
    }                                           

